{
  "name": "product-overview",
  "version": "0.1.0",
  "description": "A MERN stack app built with Redux architecture in the front-end to display products and their details.",
  "main": "server/index.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andysenclave/product-overview.git"
  },
  "author": "Anindya Mukherjee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andysenclave/product-overview/issues"
  },
  "homepage": "https://github.com/andysenclave/product-overview#readme",
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "mongodb": "^3.0.5",
    "nodemon": "^1.17.3"
  },
  "devDependencies": {}
}
